function export_results( handles , results )
% this function print the results in a CSV format


if exist('results','var')
    
    if isfield(results,'fidOut') 
        fid = results.fidOut ;
    else
        [FileName,PathName,FilterIndex] = uiputfile('*.pdf', 'Please select file to save study' );
        PDFFileName = [PathName FileName];
        fid = fopen( PDFFileName , 'a' );
    end

    warn = lastwarn ;
    if ~isempty(warn)
        fprintf( fid , '%s\n' , warn );
    else
        toBePrinted =  [ 
            results.handles.File(1:end-4) ;
            results.age ;
            results.height ;
            results.sex ;
            results.ccw ;
            results.PTPesC ;
            results.Ti ;
            results.Tt ;
            results.Vt ;
            
            results.SwingPga ;
            results.SwingPes ;
            results.PTPdiC ;
            results.CLdyn ;
            results.Wel ;
            results.Wres_insp ;
            results.Texp ;
            results.AutoPEEP ;
            results.Res
            ] ;
        for i = 1:length(toBePrinted)
           fprintf( fid , '%.3f\t' , toBePrinted ) ;
        end
        fprintf( fid , '\n' , toBePrinted ) ;
        
    end    
end